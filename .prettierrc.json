{
	"plugins": ["prettier-plugin-jsdoc"],
	"semi": false,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"printWidth": 80,
	"tabWidth": 2,
	"trailingComma": "es5",
	"bracketSameLine": false,
	"bracketSpacing": true,
	"useTabs": true,
	"singleAttributePerLine": true,
	"jsdocCommentLineStrategy": "multiline",
	"jsdocPrintWidth": 80
}
